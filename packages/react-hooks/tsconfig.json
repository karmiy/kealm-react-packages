{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "declaration": true,
    "pretty": true,
    "rootDir": "src",
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "outDir": "lib",
    "lib": [
      "es2018",
      "dom"
    ],
    "importHelpers": true,
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@kealm/react-hooks": ["src"],
    },
  },
}
